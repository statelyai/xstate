{
  "name": "@xstate/vue",
  "version": "3.0.0-beta.2",
  "description": "XState tools for Vue",
  "keywords": [
    "state",
    "machine",
    "statechart",
    "scxml",
    "state",
    "graph",
    "vue",
    "composable"
  ],
  "author": "David Khourshid <davidkpiano@gmail.com>",
  "homepage": "https://github.com/statelyai/xstate/tree/main/packages/xstate-vue#readme",
  "license": "MIT",
  "main": "dist/xstate-vue.cjs.js",
  "module": "dist/xstate-vue.esm.js",
  "exports": {
    "./fsm": {
      "module": "./fsm/dist/xstate-vue-fsm.esm.js",
      "default": "./fsm/dist/xstate-vue-fsm.cjs.js"
    },
    ".": {
      "module": "./dist/xstate-vue.esm.js",
      "default": "./dist/xstate-vue.cjs.js"
    },
    "./package.json": "./package.json"
  },
  "sideEffects": false,
  "files": [
    "dist",
    "fsm"
  ],
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/statelyai/xstate.git"
  },
  "scripts": {},
  "bugs": {
    "url": "https://github.com/statelyai/xstate/issues"
  },
  "peerDependencies": {
    "@xstate/fsm": "^3.0.0-beta.2",
    "vue": "^3.0.0",
    "xstate": "^5.0.0-beta.7"
  },
  "peerDependenciesMeta": {
    "@xstate/fsm": {
      "optional": true
    },
    "xstate": {
      "optional": true
    }
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/vue": "^6.6.1",
    "@vue/compiler-sfc": "^3.0.11",
    "@xstate/fsm": "3.0.0-beta.2",
    "vue": "^3.0.11",
    "xstate": "5.0.0-beta.7"
  },
  "preconstruct": {
    "entrypoints": [
      "./index.ts",
      "./fsm.ts"
    ]
  }
}
