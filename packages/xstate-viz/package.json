{
  "name": "@xstate/viz",
  "version": "0.1.3",
  "description": "XState visualization tools",
  "keywords": [
    "state",
    "machine",
    "statechart",
    "scxml",
    "state",
    "graph",
    "visualizer",
    "visualization",
    "react"
  ],
  "author": "David Khourshid <davidkpiano@gmail.com>",
  "homepage": "https://github.com/davidkpiano/xstate/tree/master/packages/xstate-viz#readme",
  "license": "MIT",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "sideEffects": false,
  "directories": {
    "lib": "lib",
    "test": "test"
  },
  "files": [
    "lib/**/*.js",
    "lib/**/*.d.ts",
    "themes/**/*.css"
  ],
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/davidkpiano/xstate.git"
  },
  "scripts": {
    "clean": "rm -rf dist lib tsconfig.tsbuildinfo",
    "build": "tsc",
    "watch": "yarn link:react && concurrently \"tsc -w\" \"node-sass -w ./themes/dark.scss ./themes/dark.css\"",
    "test": "jest",
    "prepublish": "npm run build && npm run unlink:react && npm run test",
    "unlink:react": "yarn unlink react && yarn unlink react-dom",
    "link:react": "yarn link react && yarn link react-dom"
  },
  "bugs": {
    "url": "https://github.com/davidkpiano/xstate/issues"
  },
  "peerDependencies": {
    "@xstate/fsm": "^1.0.0",
    "react": "^16.8.0",
    "xstate": "^4.7.0"
  },
  "peerDependenciesMeta": {
    "xstate": {
      "optional": true
    }
  },
  "devDependencies": {
    "@testing-library/react": "^8.0.9",
    "@types/jsdom": "^12.2.3",
    "@types/react": "^16.9.11",
    "@types/react-dom": "^16.9.4",
    "@xstate/fsm": "*",
    "concurrently": "^5.1.0",
    "jest": "^24.8.0",
    "jsdom": "^14.0.0",
    "jsdom-global": "^3.0.2",
    "lerna-alias": "3.0.3-0",
    "node-sass": "^4.13.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "ts-jest": "^24.1.9",
    "tsdx": "^0.11.0",
    "tslib": "^1.10.0",
    "typescript": "^3.8.3",
    "xstate": "*"
  },
  "dependencies": {
    "immer": "^5.0.0",
    "styled-components": "^4.4.1"
  }
}
