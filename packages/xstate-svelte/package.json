{
  "name": "@xstate/svelte",
  "version": "2.1.0",
  "description": "XState tools for Svelte",
  "keywords": [
    "state",
    "machine",
    "statechart",
    "scxml",
    "state",
    "graph",
    "svelte",
    "store"
  ],
  "author": "David Khourshid <davidkpiano@gmail.com>",
  "homepage": "https://github.com/statelyai/xstate/tree/main/packages/xstate-svelte#readme",
  "license": "MIT",
  "main": "lib/index.js",
  "module": "es/index.js",
  "types": "lib/index.d.ts",
  "sideEffects": false,
  "directories": {
    "lib": "lib",
    "es": "es",
    "test": "test"
  },
  "files": [
    "lib/**/*.js",
    "lib/**/*.d.ts",
    "es/**/*.js",
    "es/**/*.d.ts"
  ],
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/statelyai/xstate.git"
  },
  "scripts": {
    "clean": "rm -rf dist lib es tsconfig.tsbuildinfo",
    "build": "tsc && tsc --outDir es --module esnext",
    "test": "jest",
    "prepare": "npm run build",
    "svelte-check": "svelte-check --workspace test"
  },
  "bugs": {
    "url": "https://github.com/statelyai/xstate/issues"
  },
  "peerDependencies": {
    "@xstate/fsm": "^2.1.0",
    "svelte": "^3.24.1 || ^4",
    "xstate": "^4.38.2"
  },
  "peerDependenciesMeta": {
    "@xstate/fsm": {
      "optional": true
    },
    "xstate": {
      "optional": true
    }
  },
  "devDependencies": {
    "@testing-library/svelte": "^3.0.0",
    "@tsconfig/svelte": "^1.0.8",
    "@types/jest": "^24.0.23",
    "@xstate/fsm": "*",
    "jest": "^26.6.3",
    "lerna-alias": "3.0.3-0",
    "svelte": "^3.24.1",
    "svelte-check": "^2.4.5",
    "svelte-jester": "^1.1.2",
    "svelte-preprocess": "^4.0.10",
    "ts-jest": "^26.5.6",
    "typescript": "^4.8.4",
    "xstate": "*"
  }
}
