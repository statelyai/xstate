  "bugs": {
    "url": "https://github.com/statelyai/xstate/issues"
  },
  "peerDependencies": {
    "xstate": "^5.0.0-beta.8"
  },
  "devDependencies": {
    "strip-ansi": "^5.2.0",
    "xstate": "5.0.0-beta.8"
  },
  "dependencies": {
    "@xstate/graph": "^2.0.0-alpha.0"
  },
  "preconstruct": {
    "exports": {
      "envConditions": [
        "browser"
      ]
    }
  }
