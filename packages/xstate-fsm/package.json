{
  "name": "@xstate/fsm",
  "version": "3.0.0-beta.2",
  "description": "XState for finite state machines",
  "keywords": [
    "state",
    "machine",
    "scxml",
    "state",
    "machine",
    "finite",
    "state",
    "machine",
    "fsm",
    "automata"
  ],
  "author": "David Khourshid <davidkpiano@gmail.com>",
  "homepage": "https://github.com/statelyai/xstate/tree/main/packages/xstate-fsm#readme",
  "license": "MIT",
  "main": "dist/xstate-fsm.cjs.js",
  "module": "dist/xstate-fsm.esm.js",
  "exports": {
    ".": {
      "types": {
        "import": "./dist/xstate-fsm.cjs.mjs",
        "default": "./dist/xstate-fsm.cjs.js"
      },
      "module": "./dist/xstate-fsm.esm.js",
      "import": "./dist/xstate-fsm.cjs.mjs",
      "default": "./dist/xstate-fsm.cjs.js"
    },
    "./package.json": "./package.json"
  },
  "imports": {
    "#is-development": {
      "development": "./src/true.js",
      "default": "./src/false.js"
    }
  },
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/statelyai/xstate.git"
  },
  "scripts": {},
  "bugs": {
    "url": "https://github.com/statelyai/xstate/issues"
