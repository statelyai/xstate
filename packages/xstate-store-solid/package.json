  },
  "bugs": {
    "url": "https://github.com/statelyai/xstate/issues"
  },
  "dependencies": {
    "@xstate/store": "workspace:^"
  },
  "peerDependencies": {
    "solid-js": "^1.7.0"
  },
  "devDependencies": {
    "@solidjs/testing-library": "^0.8.10",
    "solid-js": "^1.9.7",
    "vite-plugin-solid": "^2.11.6"
