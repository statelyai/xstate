{
  "compilerOptions": {
    "target": "esnext",
    "isolatedModules": true,
    "moduleResolution": "node16",
    "module": "node16",
    "allowImportingTsExtensions": true,
    "noEmit": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "jsx": "react-jsx",
    "lib": ["es2019", "ESNext.Promise", "dom"],
    "strict": true,
    "stripInternal": true,
    "types": ["vitest/globals"]
  },
  "exclude": ["**/examples", "**/templates"]
}
